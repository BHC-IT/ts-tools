{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"exclude": [
		"**/test/*",
		"**/doc/*",
		"**/dist/*",
		"**/typings/*",
		"**/src/types/*",
		"**/src/index.ts"
	],
	"check-coverage": true,
	"per-file": true,
	"branches": 100,
	"lines": 100,
	"functions": 100,
	"statements": 100
}